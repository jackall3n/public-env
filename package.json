{
  "name": "public-env",
  "version": "1.0.0",
  "description": "Create a mesh of public environment variables across frameworks",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.js",
      "require": "./dist/next.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "test": "bun test",
    "prepublishOnly": "bun run build",
    "release": "bumpp"
  },
  "keywords": [
    "env",
    "environment",
    "vite",
    "next",
    "nextjs",
    "public",
    "variables",
    "cross-framework"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackall3n/public-env.git"
  },
  "bugs": {
    "url": "https://github.com/jackall3n/public-env/issues"
  },
  "homepage": "https://github.com/jackall3n/public-env#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.0.0",
    "bumpp": "^9.9.1",
    "next": "^15.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "peerDependencies": {
    "vite": ">=4.0.0",
    "next": ">=13.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  }
}
